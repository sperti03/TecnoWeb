{
  "name": "selfie",
  "version": "0.0.0",
  "main": "dist/backend/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/backend/server.js",
    "dev": "vite",
    "build": "vite build && cpx \"src/backend/**/*\" dist/backend && cpx package.json dist/ && cpx index.js dist/ && npm run copy-node-modules && npm run link-node-modules",
    "lint": "eslint .",
    "preview": "vite preview",
    "copy-node-modules": "ncp ./node_modules dist/node_modules",
    "link-node-modules": "symlink-dir dist/node_modules dist/backend/node_modules"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "bootstrap-icons": "^1.11.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "marked": "^14.1.3",
    "mongodb": "^6.9.0",
    "mongoose": "^8.7.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "cpx": "^1.5.0",
    "eslint": "^8.0.0",
    "ncp": "^2.0.0",
    "rimraf": "^3.0.2",
    "symlink-dir": "^4.2.0",
    "typescript": "^5.6.3",
    "vite": "^4.0.0"
  }
}
